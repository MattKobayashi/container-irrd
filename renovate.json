{
  "$schema": "https://docs.renovatebot.com/renovate-schema.json",
  "packageRules": [
    {
      "matchPackageNames": ["/^(.*/)?postgres$/"],
      "allowedVersions": "<=15"
    },
    {
      "matchPackageNames": ["/^(.*/)?node$/"],
      "allowedVersions": "<=20"
    }
  ],
  "platformAutomerge": true
}
